require! {
	'highlight/stylesheet'
}
{Stylesheet} = stylesheet



sheet = Stylesheet!

sheet.add {
	color: 'white'
	background: 'black'
	family: 'Consolas'
	height: 12
	-bold
	-italic
	-strike
	-underline
}

tokens = {
	'punctuator': color: 'red'
	'keyword': color: 'blue'
	'numeric': color: 'blue'
}

for id, style of tokens => sheet.add {id, style}


return module.exports = sheet
